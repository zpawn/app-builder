{
  "name": "@zpawn/app-builder",
  "version": "0.0.9",
  "type": "module",
  "private": false,
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "files": [
    "dist",
    "esm",
    "cjs"
  ],
  "scripts": {
    "ts": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prepare": "husky install",
    "start": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "build": "pnpm build:clean && pnpm build:esm && pnpm build:cjs && pnpm build:dist",
    "build:clean": "rimraf ./dist && rimraf ./esm && rimraf ./cjs",
    "build:dist": "tsc",
    "build:esm": "tsc --outDir ./esm",
    "build:cjs": "babel ./src --out-dir ./cjs --extensions .ts,.tsx --plugins @babel/plugin-transform-modules-commonjs"
  },
  "peerDependencies": {
    "@deskpro/deskpro-ui": "^7",
    "react": "^18",
    "react-dom": "^18",
    "react-router-dom": "^5"
  },
  "dependencies": {
    "@deskpro/app-sdk": "^3.0.20",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "lodash": "^4.17.21",
    "modern-normalize": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-resize-observer": "^1.1.1",
    "react-router-dom": "^6.22.3",
    "simplebar": "^6.2.5",
    "styled-components": "^5.3.11",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.9",
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@storybook/addon-essentials": "^8.2.6",
    "@storybook/addon-interactions": "^8.2.6",
    "@storybook/addon-links": "^8.2.6",
    "@storybook/addon-onboarding": "^8.2.6",
    "@storybook/blocks": "^8.2.6",
    "@storybook/react": "^8.2.6",
    "@storybook/react-vite": "^8.2.6",
    "@storybook/test": "^8.2.6",
    "@swc/core": "^1.4.11",
    "@swc/helpers": "^0.5.7",
    "@testing-library/react": "^14.2.2",
    "@types/lodash": "^4.17.0",
    "@types/react": "^18.2.72",
    "@types/react-dom": "^18.2.22",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@typescript/lib-dom": "npm:@types/web@^0.0.138",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "storybook": "^8.2.6",
    "typescript": "5.4.5",
    "vite": "^5.2.10"
  }
}
